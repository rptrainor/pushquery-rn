    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "pushquery",
      "icon": "./assets/icon.png",
      "backgroundColor": "#F2EEE4"
    }
  }
}
